@{
}
@model List<IChoosable>
@if (Model.Count > 0)
{
    <select id="@Model[0].getlistname()" name="@Model[0].getlistname()">

        @foreach (var item in Model)
        {
            <option value="@item.getid()">@item.getvalue()</option>

        }
    </select>
}