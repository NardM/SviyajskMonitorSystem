@{
}
@model AttributeKey

    <a asp-action="Index" asp-controller="EntityTypes" >Добавить справочник</a>

<form>
    <div class="form-group">
        <md-form-field>
        <input
        id="keyname"
        mdInput
        placeholder="Имя атрибута"
        asp-for="name" />
        </md-form-field>
    </div>

    <select id="attrtype" name="attrtype" onchange="TypeChanged()">
        <option value="0">Целое число</option>
        <option value="1">Вещественное число</option>
        <option value="2">строка</option>
        <option value="3">Справочник</option>
        <option value="4">фотографии</option>
        <option value="5">Материальныe объекты</option>
    </select>

    <div class="collapse" id="ChooseEntityTypes">
        @Html.Partial("Choose",ViewData["ETypes"])
    </div>
    <input id="addAttr" type="button" value="Добавить атрибут" onclick="addattribute()" />
</form>